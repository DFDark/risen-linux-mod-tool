{
  "name": "risen-linux-mod-tool",
  "version": "0.7.0",
  "private": true,
  "main": "public/index.js",
  "homepage": "./",
  "dependencies": {
    "@iarna/rtf-to-html": "^1.1.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.68",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "react": "^18.2.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.3",
    "react-scripts": "5.0.1",
    "read-ini-file": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.3",
    "styled-components": "^6.1.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "nf start",
    "electron": "electron .",
    "package": "yarn build && electron-builder -l -c.extraMetadata.main=build/index.js",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src"
  },
  "build": {
    "appId": "risenlinuxmodtool",
    "productName": "Risen Linux Mod Tool",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "linux": {
      "target": "appImage"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-custom-scrollbars": "^4.0.12",
    "@types/redux-logger": "^3.0.12",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-reloader": "^1.2.3",
    "foreman": "^3.0.1",
    "reselect": "^5.0.1"
  }
}
